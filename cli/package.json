{
  "name": "@omnilumen/cli",
  "description": "OmniLumen CLI streamlines Stellar development, integrating CLI tools, Soroban contract deployment, and local development environments.",
  "version": "0.1.7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OmniLumen/omnilumen.git",
    "directory": "cli"
  },
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "babel src --out-dir dist/cjs --env-name cjs",
    "build:esm": "babel src --out-dir dist/esm --env-name esm",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "test": "mocha 'test/**/*.test.js'",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "bin": {
    "omnilumen-cli": "./bin/omnilumen-cli.js",
    "lumen-cli": "./bin/omnilumen-cli.js",
    "ocli": "./bin/omnilumen-cli.js",
    "scli": "./bin/omnilumen-cli.js"
  },
  "type": "module",
  "main": "dist/esm/index.js",
  "bugs": {
    "url": "https://github.com/OmniLumen/omnilumen/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/OmniLumen/omnilumen/#readme",
  "dependencies": {
    "@omnilumen/core": "^0.1.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "chai": "^5.1.1",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1"
  },
  "keywords": [
    "omnilumen",
    "stellar",
    "soroban",
    "blockchain",
    "lumen",
    "stellar-cli",
    "stellar-quickstart",
    "web3",
    "cli",
    "llm",
    "development-suite"
  ]
}
