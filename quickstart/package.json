{
  "name": "@omnilumen/quickstart",
  "description": "OmniLumen Quickstart streamlines Stellar development, integrating quickstart tools, provides a simple way to run stellar-core and horizon locally for development and testing.",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/OmniLumen/omnilumen.git",
    "directory": "quickstart"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "esbuild src/quickstart.js --bundle --outdir=dist",
    "clean": "rimraf ./dist",
    "dev": "node src/quickstart.js",
    "test": "jest",
    "lint": "eslint . --ext .js"
  },
  "bin": {
    "omnilumen-quickstart": "./bin/omnilumen-quickstart.js",
    "lumen-quickstart": "./bin/omnilumen-quickstart.js",
    "oqs": "./bin/omnilumen-quickstart.js",
    "sqs": "./bin/omnilumen-quickstart.js"
  },
  "type": "module",
  "main": "./dist/quickstart.js",
  "dependencies": {
    "@omnilumen/core": "^0.1.30",
    "inquirer": "^10.1.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@eslint/config-array": "^0.17.1",
    "@eslint/object-schema": "^2.1.4",
    "chai": "^5.1.1",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "mocha": "^10.7.3",
    "rimraf": "^6.0.1"
  },
  "keywords": [
    "omnilumen",
    "stellar",
    "soroban",
    "stellar-quickstart",
    "web3",
    "quickstart"
  ]
}
